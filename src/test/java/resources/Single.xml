<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Single" verbose="1">

    <test name="LoginTests">
        <classes>
            <class name="com.testmonitor.beaker.LoginTests">
                <methods>
                    <include name=".*positiveLoginTest.*"/>
                    <exclude name=".*incorrect*.*"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
